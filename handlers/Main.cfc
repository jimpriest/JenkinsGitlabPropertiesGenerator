<cfcomponent output="false" singleton>	<cfproperty name="cache" inject="cachebox:default">	<cfproperty name="gitLabService" inject="gitLabService">	<cfproperty name="fileUtils" inject="FileUtils@cbcommons">	<cffunction name="index" returntype="any" output="false" hint="Generate list of projects and branches for use in Jenkins.">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">		<cfset prc.message = "Properties file generated successfully to #getSetting('propertyPath')#.">		<cfset var projectList = "">		<cfset var group = "">		<cfset var projectArray = "">		<cfset var project = "">		<cfset var cacheKey = 'project-list'>		<!--- Cache our API call to get project list - this shouldn't change that much --->		<cfif( cache.lookup(cacheKey) )>			<cfset projectList = cache.get(cacheKey)>		<cfelse>			<!--- get ugly json struct from gitlab --->			<cfset projectList = gitLabService.getProjects( getSetting('apiKey'), getSetting('gitLabURL') )>			<!--- organize it into a list of groups and projects --->			<cfset projectList = gitLabService.setProjectsToStruct( projectList )>			<!--- <cfset cache.set(cacheKey, projectList)> --->		</cfif>		<!--- loop over projects and get their branches (this is slow)			Note: we don't want this cached as it will change frequently --->		<cfset projectList = gitLabService.createPropertyList( projectList, getSetting('apiKey'), getSetting('gitLabURL') )>		<!--- write file out --->		<cfset fileUtils.saveFile( getSetting('propertyPath'), "#projectList#")>		<!--- stuff this into PRC scope so we can display in view --->		<cfset prc.propertyText = projectList>		<cfset event.setView("main/index")>	</cffunction></cfcomponent>